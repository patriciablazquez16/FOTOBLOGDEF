<p id="notice"><%= notice %></p>

<h1>Listing Comunidads</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @comunidads.each do |comunidad| %>
      <tr>
        <td><%= link_to 'Show', comunidad %></td>
        <td><%= link_to 'Edit', edit_comunidad_path(comunidad) %></td>
        <td><%= link_to 'Destroy', comunidad, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Comunidad', new_comunidad_path %>
